<Window x:Class="OKEGui.Preview"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OKEGui"
        mc:Ignorable="d"
        Title="Preview" Height="300" Width="300">
    <Grid>
        <Image x:Name="image" HorizontalAlignment="Left" VerticalAlignment="Top" Height="100" Width="100" />
        <Slider x:Name="slider" HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="10,0,0,10" Width="275" />
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="175,208,0,0" VerticalAlignment="Top" Width="75" Click="button_Click" />
    </Grid>
</Window>
